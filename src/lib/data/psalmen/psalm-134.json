  "verses": [
    {
      "number": 1,
      "lines": [
        "Looft, looft nu aller heren HEER’,",
        "Gij zijne knechten, geeft Hem eer,",
        "Gij, die desnachts Zijn huis bewaakt",
        "En voor Zijn dienst in ijver blaakt!"
      ],
      "syllables": [
        ["Looft,", "looft", "nu", "a-", "l-", "ler", "heren", "HEER’,"],
        ["Gij", "zij-", "ne", "knech-", "ten,", "geeft", "Hem", "eer,"],
        ["Gij,", "die", "des-", "nachts", "Zijn", "huis", "be-", "waakt"],
        ["En", "voor", "Zijn", "dienst", "in", "ij-", "ver", "blaakt!"]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Heft uwe handen naar omhoog,",
        "Slaat naar het Heiligdom uw oog,",
        "En knielt eerbiedig voor Hem neer:",
        "Looft, looft nu aller heren HEER’!"
      ],
      "syllables": [
        ["Heft", "uwe", "han-", "den", "naar", "o-", "m-", "hoog,"],
        ["Slaat", "naar", "het", "Hei-", "lig-", "dom", "uw", "oog,"],
        ["En", "knielt", "eer-", "bie-", "dig", "voor", "Hem", "neer:"],
        ["Looft,", "looft", "nu", "al-", "ler", "he-","ren", "HEER’!"]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Dat ’s HEEREN zegen op u daal’;",
        "Zijn gunst uit Sion u bestraal’.",
        "Hij schiep ’t heelal, Zijn Naam tereer:",
        "Looft, looft dan aller heren HEER’!"
      ],
