        ["Ja,", "elk", "zal", "hun", "den", "rug", "toe-", "ke-", "ren,"],
        ["En", "hen", "ver-", "ne-", "ren."]
      ]
    },
    {
      "number": 9,
      "lines": [
        "Dan zullen alle mensen vrezen,",
        "Het werk verheffen van den HEER’.",
        "Zijn lof verbreiden en Zijn eer,",
        "En op Zijn daân, alom geprezen,",
        "Oplettend wezen."
      ],
      "syllables": [
        ["Dan", "zul-", "len", "al-", "le", "men-", "sen", "vre-", "zen,"],
        ["Het", "werk", "ver-", "hef-", "fen", "van", "den", "HEER’."],
