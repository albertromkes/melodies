        ["Hen", "tref-", "fen,", "en", "door", "zwa-", "re", "pla-", "gen,"],
        ["Hen", "straf", "doen", "dra-", "gen."]
      ]
    },
    {
      "number": 8,
      "lines": [
        "Hun tong, die and’ ren durfd’ onteren,",
        "En ware vromen trots versmaan,",
        "Zal zelf met schande hen belaan;",
        "Ja, elk zal hun den rug toekeren,",
        "En hen verneren."
      ],
      "syllables": [
        ["Hun", "tong,", "die", "and’", "ren", "durfd’", "ont-", "e-", "ren,"],
        ["En", "wa-", "re", "vro-", "men", "trots", "ver-", "smaan,"],
