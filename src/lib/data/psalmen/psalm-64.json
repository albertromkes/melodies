  "verses": [
    {
      "number": 1,
      "lines": [
        "’t Behaag’ U, mij gehoor te geven;",
        "Ik zend mijn klaagstem tot Uw troon;",
        "O HEER’, dat zich Uw hulp vertoon.",
        "Laat mij voor ’s vijands macht niet beven;",
        "Behoed mijn leven."
      ],
      "syllables": [
        ["’t Be-", "haag’", "U,", "mij", "ge-", "hoor", "te", "ge-", "ven;"],
        ["Ik", "zend", "mijn", "klaag-", "stem", "tot", "Uw", "troon;"],
        ["O", "HEER’,", "dat", "zich", "Uw", "hulp", "ver-", "toon."],
        ["Laat", "mij", "voor", "’s vij-", "ands", "macht", "niet", "be-", "ven;"],
        ["Be-", "hoed", "mijn", "le-", "ven."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Verberg mij voor de listigheden",
        "En voor den heimelijken raad",
        "Derbozen, die, geneigd tot kwaad,",
        "Oproerig in hun doen en reden,",
        "Steeds onrecht smeden."
      ],
      "syllables": [
        ["Ver-", "berg", "mij", "voor", "de", "lis-", "tig-", "he-", "den"],
        ["En", "voor", "den", "hei-", "me-", "lij-", "ken", "raad"],
