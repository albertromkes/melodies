  "verses": [
    {
      "number": 1,
      "lines": [
        "’k Roep, HEER’, in angst tot U gevloden,",
        "Ai, haast U tot mijn hulp en red;",
        "Hoor naar de stem van mijn gebed,",
        "Daar ik U aanroep in mijn noden."
      ],
      "syllables": [
        ["’k Roep,", "HEER’,", "in", "angst", "tot", "U", "ge-", "vlo-", "den,"],
        ["Ai,", "haast", "U", "tot", "mijn", "hulp", "en", "red;"],
