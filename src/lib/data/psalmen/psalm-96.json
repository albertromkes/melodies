  "verses": [
    {
      "number": 1,
      "lines": [
        "Zingt, zingt een nieuw gezang den HEERE;",
        "Zing, aarde, zing dien God terere;",
        "Looft ’s HEEREN Naam met hart en mond;",
        "Vermeldt Zijn heil op ’t wereldrond;",
        "Dat dag aan dag Zijn roem vermeere."
      ],
      "syllables": [
        ["Zingt,", "zingt", "een", "nieuw", "ge-", "zang", "den", "HEE-", "RE;"],
        ["Zing,", "aar-", "de,", "zing", "dien", "God", "te-", "re-", "re;"],
        ["Looft", "’s HEE-", "REN", "Naam", "met", "hart", "en", "mond;"],
        ["Ver-", "meldt", "Zijn", "heil", "op", "’t we-", "reld-", "rond;"],
        ["Dat", "dag", "aan", "dag", "Zijn", "roem", "ver-", "mee-", "re."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Nu moet uw tong de heid’ nen noden;",
        "Meldt allen volken Zijn geboden;",
        "Vertelt Zijn wond’ ren en Zijn eer;",
        "Groot en prijswaardig is de HEER’,",
        "En vrees’lijk boven al de goden."
      ],
      "syllables": [
        ["Nu", "moet", "uw", "tong", "de", "heid’", "nen", "no-", "den;"],
        ["Meldt", "al-", "len", "vol-", "ken", "Zijn", "ge-", "bo-", "den;"],
        ["Ver-", "telt", "Zijn", "wond’", "ren", "en", "Zijn", "eer;"],
        ["Groot", "en", "prijs-", "waar-", "dig", "is", "de", "HEER’,"],
        ["En", "vrees-", "’lijk", "bo-", "ven", "al", "de", "go-", "den."]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Al d’ afgoôn zijn slechts ijdelheden,",
        "Maar God, Die van ons wordt beleden,",
        "Is ’t, Die de heemlen heeft gesticht",
        "En voor Zijn Godd’ lijk Aangezicht,",
        "Zet eer met majesteit haar treden."
      ],
      "syllables": [
        ["Al", "d’af-", "goôn", "zijn", "slechts", "ij-", "del-", "he-", "den,"],
        ["Maar", "God,", "Die", "van", "ons", "wordt", "be-", "le-", "den,"],
        ["Is", "’t,", "Die", "de", "heemlen", "heeft", "ge-", "sticht"],
        ["En", "voor", "Zijn", "Godd’", "lijk", "Aan-", "ge-", "zicht,"],
        ["Zet", "eer", "met", "ma-", "jes-", "teit", "haar", "tre-", "den."]
      ]
    },
    {
      "number": 4,
      "lines": [
        "Hoe blinkt het alles door vertoning",
        "Van sterkt’ en sieraad in Zijn woning!",
        "Geef dan, o allerlei geslacht,",
        "Den roem van heerlijkheid en kracht",
        "Aan Isrels groten God en Koning."
      ],
      "syllables": [
        ["Hoe", "blinkt", "het", "al-", "les", "door", "ver-", "to-", "ning"],
        ["Van", "sterkt’", "en", "sie-", "raad", "in", "Zijn", "wo-", "ning!"],
        ["Geef", "dan,", "o", "al-", "ler-", "lei", "ge-", "slacht,"],
        ["Den", "roem", "van", "heer-", "lijk-", "heid", "en", "kracht"],
        ["Aan", "Is-", "rels", "gro-", "ten", "God", "en", "Ko-", "ning."]
      ]
    },
    {
      "number": 5,
      "lines": [
        "Geeft d’ eer aan ’t eeuwig Opperwezen;",
        "Zijn Naam wordt nooit genoeg geprezen.",
        "Verheft Zijn deugden, blij te moê;",
        "Brengt in Zijn huis Hem offer toe,",
        "Hem, Die de volken moeten vrezen."
      ],
      "syllables": [
        ["Geeft", "d’eer", "aan", "’t eeu-", "wig", "Op-", "per-", "we-", "zen;"],
        ["Zijn", "Naam", "wordt", "nooit", "ge-", "noeg", "ge-", "pre-", "zen."],
        ["Ver-", "heft", "Zijn", "deug-", "den,", "blij", "te", "moê;"],
