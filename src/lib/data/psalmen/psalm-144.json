        ["Ver-", "strooi", "hen;", "zend", "Uw", "pij-", "len", "uit", "in", "’t rond;"],
        ["Ver-", "niel", "hen;", "steek", "Uw", "han-", "den", "uit", "den", "ho-", "gen;"],
