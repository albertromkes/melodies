        ["Hun", "rech-", "ter-", "hand", "wordt", "door", "de", "list", "be-", "stierd;"],
        ["Daar", "z’aan", "’t be-", "drog", "den", "rui-", "men", "teu-", "gel", "viert."],
        ["Zo", "zul-", "len", "zich,", "als", "plan-", "ten,", "o-", "n-", "ze", "zo-", "nen,"],
