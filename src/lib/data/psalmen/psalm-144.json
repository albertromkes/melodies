        ["Dat", "Gij,", "o", "HEER’,", "hem", "gun-", "sten", "wilt", "be-", "wij-", "zen,"],
        ["Dat", "Gij", "hem", "kent?", "Wat", "is", "des-", "men-", "sen", "kind,"],
        ["Dat", "Gij", "het", "acht", "en", "zo", "ge-", "trouw", "be-", "mint?"],
        ["Hij", "mag", "den", "naam", "van", "i-", "j-", "del-", "heid", "wel", "dragen;"],
