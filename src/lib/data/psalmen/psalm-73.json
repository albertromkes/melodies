  "verses": [
    {
      "number": 1,
      "lines": [
        "Ja waarlijk, God is Isrel goed,",
        "Voor hen, die rein zijn van gemoed;",
        "Hoe donker ooit Gods weg moog’ wezen,",
        "Hij ziet in gunst op die Hem vrezen.",
        "Maar ach, hoewel mijn ziel dit weet,",
        "Mijn voeten waren in mijn leed,",
        "Schier uitgeweken, en mijn treên",
        "Van ’t spoor dergodsvrucht afgegleên."
      ],
      "syllables": [
        ["Ja", "waar-", "lijk,", "God", "is", "Is-", "rel", "goed,"],
        ["Voor", "hen,", "die", "rein", "zijn", "van", "ge-", "moed;"],
        ["Hoe", "don-", "ker", "ooit", "Gods", "weg", "moog’", "we-", "zen,"],
        ["Hij", "ziet", "in", "gunst", "op", "die", "Hem", "vre-", "zen."],
        ["Maar", "ach,", "hoe-", "wel", "mijn", "ziel", "dit", "weet,"],
        ["Mijn", "voe-", "ten", "wa-", "ren", "in", "mijn", "leed,"],
        ["Schier", "uit-", "ge-", "we-", "ken,", "en", "mijn", "treên"],
        ["Van", "’t spoor", "der-", "gods-", "vrucht", "af-", "ge-", "gleên."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Ik zag met nijdig’ ogen aan,",
        "Hoe dwazen hier op rozen gaan,",
        "En hoe godd’ lozen in hun gangen,",
        "Al veeltijds rust en vreê erlangen.",
        "Zij weten van geen tranenbrood,",
        "Van gene banden tot hun dood;",
        "Hun kracht is fris; zij zijn gezond",
        "Tot op hun laatsten avondstond."
      ],
      "syllables": [
        ["Ik", "zag", "met", "nij-", "dig’", "o-", "gen", "aan,"],
        ["Hoe", "dwa-", "zen", "hier", "op", "ro-", "zen", "gaan,"],
        ["En", "hoe", "god-", "d’lo-", "zen", "in", "hun", "gan-", "gen,"],
        ["Al", "veel-", "tijds", "rust", "en", "vreê", "er-", "lan-", "gen."],
        ["Zij", "we-", "ten", "van", "geen", "tra-", "nen-", "brood,"],
        ["Van", "ge-", "ne", "ban-", "den", "tot", "hun", "dood;"],
        ["Hun", "kracht", "is", "fris;", "zij", "zijn", "ge-", "zond"],
        ["Tot", "op", "hun", "laat-", "sten", "a-", "vond-", "stond."]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Zij weten doorgaans van verdriet",
        "En moeit’, als and’ re mensen niet.",
        "Men ziet hen bitt’re smart noch plagen,",
        "Als and’ re strevelingen dragen.",
        "Dies zijn zij trots, en doen den waan,",
        "Gelijk een gouden keten aan.",
        "’t Geweld, dat deugd en plicht versmaadt,",
        "Bedekt hen als een praalgewaad."
      ],
      "syllables": [
        ["Zij", "we-", "ten", "door-", "gaans", "van", "ver-", "driet"],
        ["En", "moeit’,", "als", "and’", "re", "men-", "sen", "niet."],
        ["Men", "ziet", "hen", "bitt-", "’re", "smart", "noch", "pla-", "gen,"],
        ["Als", "and’", "re", "stre-", "ve-", "lin-", "gen", "dra-", "gen."],
        ["Dies", "zijn", "zij", "trots,", "en", "doen", "den", "waan,"],
        ["Ge-", "lijk", "een", "gou-", "den", "ke-", "ten", "aan."],
        ["’t Ge-", "weld,", "dat", "deugd", "en", "plicht", "ver-", "smaadt,"],
        ["Be-", "dekt", "hen", "als", "een", "praal-", "ge-", "waad."]
      ]
    },
    {
      "number": 4,
      "lines": [
        "Indien men op hun voorspoed let,",
        "Hun ogen puilen uit van vet;",
        "Hun weelde, wat zij zich beloven,",
        "Gaat hun verbeelding nog te boven,",
        "Zij mergelen de mensen uit,",
        "En spreken trots op roof en buit,",
        "Steeds uit de hoogte van hun macht,",
        "Terwijl hun hart de deugd belacht."
      ],
      "syllables": [
        ["In-", "dien", "men", "op", "hun", "voor-", "spoed", "let,"],
        ["Hun", "o-", "gen", "pui-", "len", "uit", "van", "vet;"],
        ["Hun", "weel-", "de,", "wat", "zij", "zich", "be-", "lo-", "ven,"],
        ["Gaat", "hun", "ver-", "beel-", "ding", "nog", "te", "bo-", "ven,"],
        ["Zij", "mer-", "ge-", "len", "de", "men-", "sen", "uit,"],
        ["En", "spre-", "ken", "trots", "op", "roof", "en", "buit,"],
        ["Steeds", "uit", "de", "hoog-", "te", "van", "hun", "macht,"],
        ["Ter-", "wijl", "hun", "hart", "de", "deugd", "be-", "lacht."]
      ]
    },
    {
      "number": 5,
      "lines": [
        "Hun mond tast zelfs den hemel aan;",
        "Gods albestuur schijnt hun een waan;",
        "Terwijl hun tong op aarde wandelt;",
        "Geen mens ontziet, maar elk mishandelt;",
        "Daarom keert zich Gods volk hiertoe,",
        "En schrikt, wanneer hun bang te moê,",
        "Het water, daar hun niets gelukt,",
        "Met bekers vol wordt uitgedrukt."
      ],
      "syllables": [
        ["Hun", "mond", "tast", "zelfs", "den", "he-", "mel", "aan;"],
        ["Gods", "al-", "be-", "stuur", "schijnt", "hun", "een", "waan;"],
        ["Ter-", "wijl", "hun", "tong", "op", "aar-", "de", "wan-", "delt;"],
        ["Geen", "mens", "ont-", "ziet,", "maar", "elk", "mis-", "han-", "delt;"],
        ["Daar-", "om", "keert", "zich", "Gods", "volk", "hier-", "toe,"],
        ["En", "schrikt,", "wan-", "neer", "hun", "bang", "te", "moê,"],
        ["Het", "wa-", "ter,", "daar", "hun", "niets", "ge-", "lukt,"],
        ["Met", "be-", "kers", "vol", "wordt", "uit-", "ge-", "drukt."]
      ]
    },
    {
      "number": 6,
      "lines": [
        "Den peinst de ziel; is ’t waar, zou God",
        "Ook weten van mijn droevig lot;",
        "Zou d’ Allerhoogste van mijn klagen",
        "En bitt’re rampen kennis dragen?",
        "Ziet dezen, hoe godd’ loos en wreed,",
        "Zijn evenwel bevrijd van leed;",
        "De rust volgt hen op al hun paân,",
        "En hun vermogen groeit steeds aan."
      ],
      "syllables": [
        ["Den", "peinst", "de", "ziel;", "is", "’t waar,", "zou", "God"],
        ["Ook", "we-", "ten", "van", "mijn", "droe-", "vig", "lot;"],
        ["Zou", "d’Al-", "ler-", "hoog-", "ste", "van", "mijn", "kla-", "gen"],
        ["En", "bitt-", "’re", "ram-", "pen", "ken-", "nis", "dra-", "gen?"],
        ["Ziet", "de-", "zen,", "hoe", "godd’", "loos", "en", "wreed,"],
        ["Zijn", "e-", "ven-", "wel", "be-", "vrijd", "van", "leed;"],
        ["De", "rust", "volgt", "hen", "op", "al", "hun", "paân,"],
        ["En", "hun", "ver-", "mo-", "gen", "groeit", "steeds", "aan."]
      ]
    },
    {
      "number": 7,
      "lines": [
        "Zo heb ik dan vergeefs gestreên,",
        "Mijn hart gezuiverd en gebeên;",
        "Vergeefs heb ik in reine plassen",
        "Van onschuld mijne hand gewassen.",
        "Want al den dag ben ik geplaagd;",
        "Mijn ziel verschrikt, mijn boezem jaagt;",
        "En nooit verscheen ermorgenstond,",
        "Waarop ik geen kastijding vond."
      ],
      "syllables": [
        ["Zo", "heb", "ik", "dan", "Ver-", "geefs", "ge-", "streên,"],
        ["Mijn", "hart", "ge-", "zui-", "verd", "en", "ge-", "beên;"],
        ["Ver-", "geefs", "heb", "ik", "in", "rei-", "ne", "plas-", "sen"],
        ["Van", "on-", "schuld", "mij-", "ne", "hand", "ge-", "was-", "sen."],
        ["Want", "al", "den", "dag", "ben", "ik", "ge-", "plaagd;"],
        ["Mijn", "ziel", "ver-", "schrikt,", "mijn", "boe-", "zem", "jaagt;"],
        ["En", "nooit", "ver-", "scheen", "er-", "mor-", "gen-", "stond,"],
        ["Waar-", "op", "ik", "geen", "kas-", "tij-", "ding", "vond."]
      ]
    },
    {
      "number": 8,
      "lines": [
        "Zo ik dit zeggen staven zou,",
        "Gewis, dan waar’ ik niet getrouw",
        "Aan ’t waard geslacht van Uwe kind’ ren",
        "En zou hun hoop en moed vermind’ ren;",
        "Nochtans heb ik met al mijn kracht",
        "De Godsregering overdacht,",
        "Maar ’t was een stuk, dat in mijn oog,",
        "Mij moeilijk viel en veel te hoog."
      ],
      "syllables": [
        ["Zo", "ik", "dit", "zeg-", "gen", "sta-", "ven", "zou,"],
        ["Ge-", "wis,", "dan", "waar’", "ik", "niet", "ge-", "trouw"],
        ["Aan", "’t waard", "ge-", "slacht", "van", "U-", "we", "kind’", "ren"],
        ["En", "zou", "hun", "hoop", "en", "moed", "ver-", "mind’", "ren;"],
        ["Noch-", "tans", "heb", "ik", "met", "al", "mijn", "kracht"],
        ["De", "Gods-", "re-", "ge-", "ring", "o-", "ver-", "dacht,"],
        ["Maar", "’t was", "een", "stuk,", "dat", "in", "mijn", "oog,"],
        ["Mij", "moei-", "lijk", "viel", "en", "veel", "te", "hoog."]
      ]
    },
    {
      "number": 9,
      "lines": [
        "Dit duurde tot ik uit dien drom",
        "Van neev’len ging in ’t heiligdom,",
        "Om met de Godsspraak raad te plegen.",
        "Daar zag ik, op wat gladde wegen",
        "De voorspoed zelfs de bozen leidt;",
        "En hoe G’in ’t eind hun val bereidt;",
        "Zij storten van den top van eer",
        "In eeuwige verwoesting neer."
      ],
      "syllables": [
        ["Dit", "duur-", "de", "tot", "ik", "uit", "dien", "drom"],
        ["Van", "neev-", "’len", "ging", "in", "’t hei-", "lig-", "dom,"],
        ["Om", "met", "de", "Gods-", "spraak", "raad", "te", "ple-", "gen."],
        ["Daar", "zag", "ik,", "op", "wat", "glad-", "de", "we-", "gen"],
        ["De", "voor-", "spoed", "zelfs", "de", "bo-", "zen", "leidt;"],
        ["En", "hoe", "G’in", "’t eind", "hun", "val", "be-", "reidt;"],
        ["Zij", "stor-", "ten", "van", "den", "top", "van", "eer"],
        ["In", "eeu-", "wi-", "ge", "ver-", "woes-", "ting", "neer."]
      ]
    },
    {
      "number": 10,
      "lines": [
        "Hoe worden zij tot ieders schrik",
        "Vernield als in een ogenblik;",
        "Hoe moeten zij het leven enden,",
        "Van angst verteerd in hun ellenden?",
        "Hun weeld’ is als een droom vergaan.",
        "O HEER’, wanneer Gij op zult staan,",
        "Zult Gij hun tonen, onverwacht,",
        "Hoe Gij hun ijdel beeld veracht."
      ],
      "syllables": [
        ["Hoe", "wor-", "den", "zij", "tot", "ie-", "ders", "schrik"],
        ["Ver-", "nield", "als", "in", "een", "o-", "gen-", "blik;"],
        ["Hoe", "moe-", "ten", "zij", "het", "le-", "ven", "en-", "den,"],
        ["Van", "angst", "ver-", "teerd", "in", "hun", "el-", "len-", "den?"],
        ["Hun", "weeld’", "is", "als", "een", "droom", "ver-", "gaan."],
