        ["Zult", "G’uit", "den", "dood", "ons", "niet", "her-", "le-", "ven", "doen,"],
        ["Op-", "dat", "Uw", "volk", "zich", "weer", "in", "U", "ver-", "blij’."],
        ["Dat", "toch,", "o", "HEER’,", "Uw", "goed-", "heid", "ons", "be-", "vrij’."],
        ["Geef", "ons", "Uw", "heil,", "en", "red", "door", "U-", "we", "hand,"],
