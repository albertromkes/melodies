        ["Ge-", "bit", "en", "toom,", "door", "’s men-", "sen", "hand", "be-", "stierd,"],
        ["Be-", "teug-", "’len", "’t woest", "en", "re-", "de-", "loos", "ge-", "diert’."],
        ["Laat", "zulk", "een", "dwang", "voor", "u", "niet", "no-", "dig", "we-", "zen;"],
        ["Wie", "God", "ver-", "laat,", "heeft", "smart", "op", "smart", "te", "vre-", "zen;"],
        ["Maar", "wie", "op", "Hem", "ver-", "trouwt,", "op", "Hem", "al-", "leen,"],
        ["Ziet", "zich", "om-", "ringd", "met", "Zijn", "wel-", "da-", "dig-", "heên."]
      ]
    },
    {
      "number": 6,
      "lines": [
        "Rechtvaardig volk, verhef uw blijde klanken,",
        "Verheugd in God, naar waarde nooit te danken.",
        "Zingt vrolijk, roemt Zijn deugden t’allen tijd,",
        "Gij, die oprecht van hart en wandel zijt."
      ],
