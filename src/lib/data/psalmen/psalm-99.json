        ["Die", "door", "’s HEE-", "REN", "kracht,"],
        ["Van", "hen", "werd", "vol-", "bracht."],
        ["’t Na-", "kroost", "der", "He-", "breeu-", "wen"],
        ["Vol-", "ge", "dit", "all’", "eeu-", "wen."]
      ]
    },
    {
      "number": 7,
      "lines": [
        "Gij, met hen begaan,",
        "Hebt hun wens voldaan;",
        "HEER’, die naar Uw woord,",
        "Hun gebed verhoort,",
        "Gij, Gij waart hun lot,",
        "Hun vergevend God;",
        "Schoon z’ook om hun zonden,",
        "Straffen ondervonden."
      ],
      "syllables": [
        ["Gij,", "met", "hen", "be-", "gaan,"],
