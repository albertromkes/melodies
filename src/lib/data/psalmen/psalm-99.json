  "verses": [
    {
      "number": 1,
      "lines": [
        "God, de HEER’, regeert.",
        "Beeft, gij volken eert,",
        "Eert Zijn hoog bestel,",
        "Die bij Israël,",
        "Tussen Cherubs woont",
        "En Zijn grootheid toont;",
        "Dat zich d’ aard bewege.",
        "Hij is Isrels zege!"
      ],
      "syllables": [
        ["God,", "de", "HEER’,", "re-", "geert."],
        ["Beeft,", "gij", "vol-", "ken", "eert,"],
        ["Eert", "Zijn", "hoog", "be-", "stel,"],
