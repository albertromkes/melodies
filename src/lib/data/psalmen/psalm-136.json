        ["Ga-", "de-", "lo-", "ze", "wond’", "ren", "werkt;"],
        ["Want", "Zijn", "gunst,", "a-", "l-", "om", "ver-", "spreid,"],
        ["Zal", "be-", "staan", "in", "eeu-", "wig-", "heid."]
      ]
    },
    {
      "number": 5,
      "lines": [
        "Looft Gods wijsheid; door Zijn woord",
        "Bracht Hij al de heemlen voort;",
        "Want Zijn gunst, alom verspreid,",
        "Zal bestaan in eeuwigheid."
      ],
      "syllables": [
        ["Looft", "Gods", "wijs-", "heid;", "door", "Zijn", "woord"],
        ["Bracht", "Hij", "al", "de", "heem-", "len", "voort;"],
        ["Want", "Zijn", "gunst,", "a-", "l-", "om", "ver-", "spreid,"],
        ["Zal", "be-", "staan", "in", "eeu-", "wig-", "heid."]
      ]
    },
    {
      "number": 6,
      "lines": [
        "D’ aard’ hief uit derwaat’ren schoot",
        "Zich omhoog, toen God ’t gebood;",
        "Want Zijn gunst, alom verspreid,",
        "Zal bestaan in eeuwigheid."
