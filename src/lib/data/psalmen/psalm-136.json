  "verses": [
    {
      "number": 1,
      "lines": [
        "Looft den HEER’, want Hij is goed,",
        "Looft Hem met een blij gemoed;",
        "Want Zijn gunst, alom verspreid,",
        "Zal bestaan in eeuwigheid."
      ],
      "syllables": [
        ["Looft", "den", "HEER’,", "want", "Hij", "is", "goed,"],
        ["Looft", "Hem", "met", "een", "blij", "ge-", "moed;"],
        ["Want", "Zijn", "gunst,", "a-", "l-", "om", "ver-", "spreid,"],
        ["Zal", "be-", "staan", "in", "eeu-", "wig-", "heid."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Looft den groten God, Wiens troon",
        "Hoger rijst dan die dergoôn;",
        "Want Zijn gunst, alom verspreid,",
        "Zal bestaan in eeuwigheid."
      ],
      "syllables": [
        ["Looft", "den", "gro-", "ten", "God,", "Wiens", "troon"],
        ["Ho-", "ger", "rijst", "dan", "die", "der-", "goôn;"],
