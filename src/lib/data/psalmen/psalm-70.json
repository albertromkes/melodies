  "verses": [
    {
      "number": 1,
      "lines": [
        "Daal haastig terverlossing neer,",
        "O God, en red mij uit gevaren,",
        "Uit angsten, die mijn ziel bezwaren,",
        "Spoed U te mijner hulp, o HEER’.",
        "Laat allen, die mijn ziel belagen,",
        "Beschaamd en schaamrood van mij vliên;",
        "Laat, die met vreugd mijn rampen zien,",
        "In hunne wensen nimmer slagen."
      ],
      "syllables": [
        ["Daal", "haas-", "tig", "ter-", "ver-", "los-", "sing", "neer,"],
        ["O", "God,", "en", "red", "mij", "uit", "ge-", "va-", "ren,"],
        ["Uit", "a-", "ng-", "sten,", "die", "mijn", "ziel", "be-", "zwa-", "ren,"],
        ["Spoed", "U", "te", "mij-", "ner", "hulp,", "o", "HEER’."],
        ["Laat", "a-", "l-", "len,", "die", "mijn", "ziel", "be-", "la-", "gen,"],
        ["Be-", "schaamd", "en", "schaam-", "rood", "van", "mij", "vliên;"],
        ["Laat,", "die", "met", "vreugd", "mijn", "ram-", "pen", "zien,"],
        ["In", "hun-", "ne", "wen-", "sen", "nim-", "mer", "sla-", "gen."]
