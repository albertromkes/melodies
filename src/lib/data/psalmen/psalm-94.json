  "verses": [
    {
      "number": 1,
      "lines": [
        "Verschijn nu blinkend, God derwrake;",
        "Dat eens Uw arm voor ons ontwake.",
        "Vertoon Uw glansrijk aangezicht;",
        "Gij, Rechter, die de wereld richt,",
        "Sta op, verhef U en vergeld,",
        "Hovaardigen hun trots geweld."
      ],
      "syllables": [
        ["Ver-", "schijn", "nu", "blin-", "kend,", "God", "der-", "wra-", "ke;"],
        ["Dat", "eens", "Uw", "arm", "voor", "ons", "ont-", "wa-", "ke."],
        ["Ver-", "toon", "Uw", "glans-", "rijk", "aan-", "ge-", "zicht;"],
        ["Gij,", "Rech-", "ter,", "die", "de", "we-", "reld", "richt,"],
        ["Sta", "op,", "ver-", "hef", "U", "en", "ver-", "geld,"],
        ["Ho-", "vaar-", "di-", "gen", "hun", "trots", "ge-", "weld."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Hoe lang, HEER’, zullen dan de bozen,",
        "Hoe langen tijd de goddelozen",
        "Nog hupp’len, vol van dart’le vreugd,",
        "En laster braken op de deugd,",
        "En spreken als in zegepraal,",
        "Baldadig d’ allerhardste taal?"
      ],
      "syllables": [
        ["Hoe", "lang,", "HEER’,", "zul-", "len", "dan", "de", "bo-", "zen,"],
        ["Hoe", "lan-", "gen", "tijd", "de", "god-", "de-", "lo-", "zen"],
        ["Nog", "hupp-", "’len,", "vol", "van", "dart-", "’le", "vreugd,"],
        ["En", "las-", "ter", "bra-", "ken", "op", "de", "deugd,"],
        ["En", "spre-", "ken", "als", "in", "ze-", "ge-", "praal,"],
        ["Bal-", "da-", "dig", "d’al-", "ler-", "hard-", "ste", "taal?"]
      ]
    },
    {
      "number": 3,
      "lines": [
        "’t Verbrijzeld volk, o HEER’, moet bukken,",
        "Daar zij Uw erfdeel wreed verdrukken;",
        "De zwakke weeuw, van hulp ontbloot,",
        "Wordt met den vreemdeling gedood.",
        "Zelfs wordt d’ onnooz’le wees vermoord;",
        "Naar recht noch reden wordt gehoord."
      ],
      "syllables": [
        ["’t Ver-", "brij-", "zeld", "volk,", "o", "HEER’,", "moet", "buk-", "ken,"],
        ["Daar", "zij", "Uw", "erf-", "deel", "wreed", "ver-", "druk-", "ken;"],
        ["De", "zwak-", "ke", "weeuw,", "van", "hulp", "ont-", "bloot,"],
        ["Wordt", "met", "den", "vreem-", "de-", "ling", "ge-", "dood."],
        ["Zelfs", "wordt", "d’on-", "nooz-", "’le", "wees", "ver-", "moord;"],
        ["Naar", "recht", "noch", "re-", "den", "wordt", "ge-", "hoord."]
      ]
    },
    {
      "number": 4,
      "lines": [
        "Zij zeggen, stout op hun vermogen:",
        "„De HEER’ slaat op ons doen geen ogen,",
        "De God van Jakob merkt het niet.”",
        "Let, onvernuftigen, en ziet;",
        "Blijft g’eeuwig van verstand beroofd,",
        "Gij, dwazen, die het licht verdooft?"
      ],
      "syllables": [
        ["Zij", "zeg-", "gen,", "stout", "op", "hun", "ver-", "mo-", "gen:"],
        ["„De", "HEER’", "slaat", "op", "ons", "doen", "geen", "o-", "gen,"],
        ["De", "God", "van", "Ja-", "kob", "merkt", "het", "niet.”"],
        ["Let,", "on-", "ver-", "nuf-", "ti-", "gen,", "en", "ziet;"],
        ["Blijft", "g-", "’eeuwig", "van", "ver-", "stand", "be-", "roofd,"],
        ["Gij,", "dwa-", "zen,", "die", "het", "licht", "ver-", "dooft?"]
      ]
    },
    {
      "number": 5,
      "lines": [
        "Zou dan de Schepper, die onz’ oren,",
        "Geplant heeft, Zelf niet kunnen horen?",
        "Zou Hij, die ’t oog formeert, niet zien?",
        "Zoudt Gij des Rechters wraak ontvliên,",
        "Die volken straft, en wijsheid leert",
        "Den mens, die wetenschap ontbeert?"
      ],
      "syllables": [
        ["Zou", "dan", "de", "Schep-", "per,", "die", "onz’", "o-", "ren,"],
        ["Ge-", "plant", "heeft,", "Zelf", "niet", "kun-", "nen", "ho-", "ren?"],
        ["Zou", "Hij,", "die", "’t oog", "for-", "meert,", "niet", "zien?"],
        ["Zoudt", "Gij", "des", "Rech-", "ters", "wraak", "ont-", "vliên,"],
        ["Die", "vol-", "ken", "straft,", "en", "wijs-", "heid", "leert"],
        ["Den", "mens,", "die", "we-", "ten-", "schap", "ont-", "beert?"]
      ]
    },
    {
      "number": 6,
      "lines": [
        "Neen, dwaas, de HEER’ weet uw gedachten,",
        "Dat z’ijdel Zijn bestuur verachten.",
        "Welzalig is de man, o HEER’,",
        "Die door Uw tucht en hemelleer",
        "Het nut deronderdrukking weet,",
        "En voordeel trekt zelfs uit het leed."
      ],
      "syllables": [
        ["Neen,", "dwaas,", "de", "HEER’", "weet", "uw", "ge-", "dach-", "ten,"],
        ["Dat", "z-", "’ijdel", "Zijn", "be-", "stuur", "ver-", "ach-", "ten."],
        ["Wel-", "za-", "lig", "is", "de", "man,", "o", "HEER’,"],
        ["Die", "door", "Uw", "tucht", "en", "he-", "mel-", "leer"],
        ["Het", "nut", "de-", "ron-", "der-", "druk-", "king", "weet,"],
        ["En", "voor-", "deel", "trekt", "zelfs", "uit", "het", "leed."]
      ]
    },
    {
      "number": 7,
      "lines": [
        "Zo leert hij zich geduldig dragen,",
        "Zo ziet hij ’t eind derkwade dagen;",
        "Zo wordt de roede zelfs gekust,",
        "En d’ onderwerping geeft hem rust,",
        "Totdat de kuil gegraven wordt,",
        "Waarin de zondaar nederstort."
      ],
      "syllables": [
        ["Zo", "leert", "hij", "zich", "ge-", "dul-", "dig", "dra-", "gen,"],
        ["Zo", "ziet", "hij", "’t eind", "der-", "kwa-", "de", "da-", "gen;"],
        ["Zo", "wordt", "de", "roe-", "de", "zelfs", "ge-", "kust,"],
        ["En", "d’on-", "der-", "wer-", "ping", "geeft", "hem", "rust,"],
        ["Tot-", "dat", "de", "kuil", "ge-", "gra-", "ven", "wordt,"],
        ["Waar-", "in", "de", "zon-", "daar", "ne-", "der-", "stort."]
      ]
    },
    {
      "number": 8,
      "lines": [
        "De HEER’ zal in dit moeilijk leven,",
        "Zijn volk en erfdeel nooit begeven.",
        "Het oordeel keert, vol majesteit,",
        "Haast weder tot gerechtigheid;",
        "Al wie oprecht is van gemoed,",
        "Die merkt het op, en keurt het goed."
      ],
      "syllables": [
        ["De", "HEER’", "zal", "in", "dit", "moei-", "lijk", "le-", "ven,"],
        ["Zijn", "volk", "en", "erf-", "deel", "nooit", "be-", "ge-", "ven."],
        ["Het", "oor-", "deel", "keert,", "vol", "ma-", "jes-", "teit,"],
        ["Haast", "we-", "der", "tot", "ge-", "rech-", "tig-", "heid;"],
        ["Al", "wie", "op-", "recht", "is", "van", "ge-", "moed,"],
        ["Die", "merkt", "het", "op,", "en", "keurt", "het", "goed."]
      ]
    },
    {
      "number": 9,
      "lines": [
        "Wie helpt mij tegen al die bozen?",
        "Wie wederstaat die goddelozen?",
        "Zo mij de HEER’, mijn schild en loon",
        "Geen sterken bijstand had geboôn,",
        "Dan waar’ mijn leven haast verkort,",
        "En ik bijna in ’t graf gestort."
      ],
      "syllables": [
        ["Wie", "helpt", "mij", "te-", "gen", "al", "die", "bo-", "zen?"],
        ["Wie", "we-", "der-", "staat", "die", "god-", "de-", "lo-", "zen?"],
        ["Zo", "mij", "de", "HEER’,", "mijn", "schild", "en", "loon"],
        ["Geen", "ster-", "ken", "bij-", "stand", "had", "ge-", "boôn,"],
        ["Dan", "waar’", "mijn", "le-", "ven", "haast", "ver-", "kort,"],
        ["En", "ik", "bij-", "na", "in", "’t graf", "ge-", "stort."]
      ]
    },
    {
      "number": 10,
      "lines": [
        "Wanneer ik zei: „Mijn voeten glijden”,",
        "Toen hebt Gij mij gesterkt in ’t lijden;",
        "Wanneer mij ’t afgepeinsde hart,",
        "Door al mijn denken werd verward;",
        "En ik in druk schier was gestikt,",
        "Toen heeft Uw troost mijn ziel verkwikt."
      ],
      "syllables": [
        ["Wan-", "neer", "ik", "zei:", "„Mijn", "voe-", "ten", "glij-", "den”,"],
        ["Toen", "hebt", "Gij", "mij", "ge-", "sterkt", "in", "’t lij-", "den;"],
        ["Wan-", "neer", "mij", "’t af-", "ge-", "peins-", "de", "hart,"],
        ["Door", "al", "mijn", "den-", "ken", "werd", "ver-", "ward;"],
        ["En", "ik", "in", "druk", "schier", "was", "ge-", "stikt,"],
        ["Toen", "heeft", "Uw", "troost", "mijn", "ziel", "ver-", "kwikt."]
      ]
    },
    {
      "number": 11,
      "lines": [
        "Zou ooit de stoel derschand’ lijkheden",
        "Bij Uwen troon een plaats bekleden;",
        "Die moeit’ en wetten boos verdicht?",
        "Zij rotten saam, en, wars van ’t licht,",
        "Verdrukken zij het vroom gemoed,",
        "Ja, doemen zelfs ’t onschuldig bloed."
      ],
      "syllables": [
        ["Zou", "ooit", "de", "stoel", "der-", "schand’", "lijk-", "he-", "den"],
        ["Bij", "U-", "wen", "troon", "een", "plaats", "be-", "kle-", "den;"],
