      "syllables": [
        ["Her-", "denk", "de", "trouw,", "aan", "ons", "voor-", "heen", "be-", "toond;"],
        ["Denk", "aan", "Uw", "volk,", "door", "U", "van", "ouds", "ver-", "kre-", "gen;"],
