      "syllables": [
        ["Ruk", "spoe-", "dig", "aan,", "ver-", "dub-", "beld", "U-", "we", "schreên;"],
        ["Zie,", "hoe", "de", "stad", "ver-", "woest", "ligt", "en", "ver-", "ge-", "ten;"],
