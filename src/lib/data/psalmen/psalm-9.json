        ["Dien", "wilde", "Hij", "o-", "n-", "wrik-", "baar", "stich-", "ten,"],
        ["Om", "naar", "het", "hei-", "lig", "recht", "te", "rich-", "ten."]
      ]
    },
    {
      "number": 8,
      "lines": [
