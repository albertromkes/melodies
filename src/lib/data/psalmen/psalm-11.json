        ["Ziet", "van", "Zijn", "troon", "o-", "p-", "let-", "tend", "naar", "be-", "ne-", "den:"],
        ["Hij,", "die", "nooit", "duldt,", "dat", "d’o-", "n-", "schuld", "wordt", "verdrukt,"],
        ["Proeft", "elks", "ge-", "drag,", "zelfs", "met", "Zijn", "o-", "gen-", "le-", "den."]
      ]
