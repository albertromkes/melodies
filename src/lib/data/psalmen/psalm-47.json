    {
      "number": 2,
      "lines": [
        "Naar Gods wijs bestel,",
        "Op Gods hoog bevel,",
        "Slaan wij, door Zijn hand,",
        "Volken aan den band.",
        "Die, door ons verneerd,",
        "Door ons overheerd,",
        "Strekken tot een blijk,",
        "Hoe Hij, liefderijk,",
        "Aan Zijn woord gedenkt;",
        "D’ erfenis ons schenkt,",
        "Jakobs heerlijkheid,",
        "Aan hem toegezeid."
      ],
      "syllables": [
        ["Naar", "Gods", "wijs", "be-", "stel,"],
        ["Op", "Gods", "hoog", "be-", "vel,"],
