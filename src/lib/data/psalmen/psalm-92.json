        ["Wie", "on-", "recht", "durft", "be-", "gaan."],
        ["Zult", "Gij,", "o", "God,", "weer-", "staan,"],
        ["Ver-", "strooi-", "en", "en", "doen", "sne-", "ven."],
        ["Gij", "zult", "mijn", "eer", "ver-", "gro-", "ten,"],
