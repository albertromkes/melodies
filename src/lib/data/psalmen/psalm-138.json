        ["Want", "groot", "is", "’s HEE-", "REN", "heer-", "lijk-", "heid,"],
        ["Zijn", "Ma-", "jes-", "teit"],
        ["Ten", "top", "ge-", "ste-", "gen."],
        ["Hij", "slaat", "toch,", "schoon", "o-", "n-", "ein-", "dig", "hoog,"],
        ["Op", "hen", "het", "oog,"],
        ["Die", "need’", "rig", "knie-",  "-", "len;"],
