  "verses": [
    {
      "number": 1,
      "lines": [
        "’k Zal met mijn ganse hart Uw eer",
        "Vermelden, HEER’,",
        "U dank bewijzen.",
        "’k Zal U in ’t midden van de goôn,",
        "Op hogen toon,",
        "Met psalmen prijzen;",
        "Ik zal mij buigen, op Uw eis,",
        "Naar Uw paleis,",
        "Het hof derhoven,",
        "En, om Uw gunst en waarheid saam,",
        "Uw groten Naam",
        "Eerbiedig loven."
      ],
      "syllables": [
        ["’k Zal", "met", "mijn", "gan-", "se", "hart", "Uw", "eer"],
        ["Ver-", "mel-", "den,", "HEER’,"],
        ["U", "dank", "be-", "wij-", "zen."],
        ["’k Zal", "U", "in", "’t mid-", "den", "van", "de", "goôn,"],
        ["Op", "ho-", "gen", "toon,"],
        ["Met", "psal-", "men", "prij-", "zen;"],
        ["Ik", "zal", "mij", "bui-", "gen,", "op", "Uw", "eis,"],
        ["Naar", "Uw", "pa-", "leis,"],
        ["Het", "hof", "der-", "ho-", "-", "ven,"],
