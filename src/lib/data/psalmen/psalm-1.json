  "verses": [
    {
      "number": 1,
      "lines": [
        "Welzalig hij, die in der bozen raad,",
        "Niet wandelt, noch op ’t pad der zondaars staat.",
        "Noch nederzit, daar zulken samenrotten,",
        "Die roekeloos met God en godsdienst spotten;",
        "Maar ’s HEEREN wet blijmoedig dag en nacht",
        "Herdenkt, bepeinst en ijverig betracht."
      ],
      "syllables": [
        ["Wel-", "za-", "lig", "hij,", "die", "in", "der-", "bo-", "zen", "raad,"],
        ["Niet", "wan-", "delt,", "noch", "op", "’t pad", "der-", "zon-", "daars", "staat."],
        ["Noch", "ne-", "der-", "zit,", "daar", "zul-", "ken", "sa-", "men-", "rot-", "ten,"],
        ["Die", "roe-", "ke-", "loos", "met", "God", "en", "gods-", "dienst", "spot-", "ten;"],
        ["Maar", "’s HEE-", "REN", "wet", "blij-", "moe-", "dig", "dag", "en", "nacht"],
        ["Her-", "denkt,", "be-", "peinst", "en", "ij-", "ve-", "rig", "be-", "tracht."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Want hij zal zijn gelijk een frisse boom,",
        "In vetten grond geplant bij enen stroom,",
        "Die op zijn tijd met vruchten is beladen,",
        "En sierlijk pronkt met onverwelkte bladen.",
        "Hij groeit zelfs op in ramp en tegenspoed.",
        "Het gaat hem wel, ’t gelukt hem wat hij doet."
      ],
      "syllables": [
        ["Want", "hij", "zal", "zijn", "ge-", "lijk", "een", "fris-", "se", "boom,"],
        ["In", "vet-", "ten", "grond", "ge-", "plant", "bij", "e-", "nen", "stroom,"],
        ["Die", "op", "zijn", "tijd", "met", "vruch-", "ten", "is", "be-", "la-", "den,"],
        ["En", "sier-", "lijk", "pronkt", "met", "on-", "ver-", "welk-", "te", "bla-", "den."],
        ["Hij", "groeit", "zelfs", "op", "in", "ramp", "en", "te-", "gen-", "spoed."],
        ["Het", "gaat", "hem", "wel,", "’t ge-", "lukt", "hem", "wat", "hij", "doet."]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Gans anders is ’t met hem, die ’t kwaad bemint:",
        "Hij is als kaf, dat wegstuift voor den wind.",
        "Geen zondaar zal ’t gewis verderf ontkomen,",
        "Als in ’t gericht door God wordt wraak genomen.",
        "Hij, die van deugd en godsvrucht is ontaard,",
        "Zal niet bestaan, waar ’t vrome volk vergaart."
      ],
      "syllables": [
        ["Gans", "an-", "ders", "is", "’t met", "hem,", "die", "’t kwaad", "be-", "mint:"],
