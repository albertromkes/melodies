  "verses": [
    {
      "number": 1,
      "lines": [
        "O HEER’, verlos mij uit de banden,",
        "Waarin de boze mij beknelt;",
        "Behoed mij voor deswreedaards handen,",
        "Voor dwinglandij en woest geweld."
      ],
      "syllables": [
        ["O", "HEER’,", "ver-", "los", "mij", "uit", "de", "ban-", "den,"],
        ["Waar-", "in", "de", "bo-", "ze", "mij", "be-", "knelt;"],
