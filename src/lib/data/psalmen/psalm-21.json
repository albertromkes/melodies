  "verses": [
    {
      "number": 1,
      "lines": [
        "O HEER’, de Koning is verheugd",
        "Om Uw geducht vermogen;",
        "Uw heil zweeft hem voor d’ ogen,",
        "En met wat blijde zielevreugd",
        "Zal hij, door al Uw daân.",
        "Verrukt, ten reie gaan!"
      ],
      "syllables": [
        ["O", "HEER’,", "de", "Ko-", "ning", "is", "ver-", "heugd"],
