        ["Ook", "hebt", "Gij", "d’uit-", "spraak", "van", "zijn", "mond,"],
        ["Al", "wat", "hij", "heeft", "be-", "geerd;"],
        ["Ge-", "wei-", "gerd,", "noch", "ge-", "weerd."]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Gij, die hem gunstig hebt gered,",
        "Zijt hem met volle stromen",
        "Van zegen voorgekomen.",
        "Ook hebt Gij hem op ’t hoofd gezet,",
        "Hem, die op U betrouwt,",
        "Een kroon van ’t fijnste goud."
      ],
      "syllables": [
        ["Gij,", "die", "hem", "gun-", "stig", "hebt", "ge-", "red,"],
        ["Zijt", "hem", "met", "vol-", "le", "stro-", "men"],
        ["Van", "ze-", "gen", "voor-", "ge-", "ko-", "men."],
        ["Ook", "hebt", "Gij", "hem", "op", "’t hoofd", "ge-", "zet,"],
        ["Hem,", "die", "op", "U", "be-", "trouwt,"],
        ["Een", "kroon", "van", "’t fijn-", "ste", "goud."]
