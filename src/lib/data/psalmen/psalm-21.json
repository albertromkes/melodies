    },
    {
      "number": 4,
      "lines": [
        "Hij heeft, O God, van U begeerd",
        "Het onvergank’lijk leven;",
        "Gij hebt het hem gegeven.",
        "Zo zijn de dagen hem vermeerd,",
        "Zo leeft de Vorst altoos,",
        "Zo leeft hij eindeloos."
      ],
      "syllables": [
        ["Hij", "heeft,", "O", "God,", "van", "U", "be-", "geerd"],
