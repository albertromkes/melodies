        ["Ver-", "wach-", "ten;", "laat", "geen", "schan-", "de", "we-", "der-", "va-", "ren"],
        ["Aan", "hen,", "die", "U", "steeds", "zoe-", "ken", "in", "ver-", "driet."],
        ["Met", "mij", "ver-", "ging", "hun", "hoop,", "o", "Is-", "rels", "God,"],
