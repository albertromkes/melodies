        ["Maar,", "HEER’,", "tot", "U,", "tot", "U", "is", "mijn", "ge-", "bed;"],
        ["Daar", "is,", "o", "God,", "een", "tijd", "van", "wel-", "be-", "ha-", "gen,"],
