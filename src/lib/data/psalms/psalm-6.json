    },
    {
      "number": 7,
      "lines": [
        "Mijn oog is rood gekreten,",
        "Van tranen uitgebeten,",
        "Verouderd en doorknaagd;",
        "Daar ik, in mijn ellenden,",
        "Door al mijns vijands benden,",
        "Verdrukt word en gejaagd."
      ],
      "syllables": [
        ["Mijn", "oog", "is", "rood", "ge-", "kre-", "-", "-", "ten,"],
        ["Van", "tra-", "nen", "uit-", "ge-", "be-", "ten,"],
