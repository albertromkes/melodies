      "number": 1,
      "lines": [
        "O HEER’, Gij zijt weldadig;",
        "Straf mij niet ongenadig",
        "In Uwen toornegloed,",
        "Ai, matig Uw kastijden;",
        "Sla mij met medelijden,",
        "Gelijk een vader doet."
      ],
      "syllables": [
        ["O", "HEER’,", "Gij", "zijt", "wel-", "da-", "-", "-","dig;"],
