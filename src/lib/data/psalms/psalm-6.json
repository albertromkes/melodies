        ["Schrikt", "voor", "Uw", "hei-", "lig’", "o-", "gen,"],
        ["In", "de-", "zen", "jam-", "mer-", "staat."],
        ["Hoe", "lang", "zal", "ik", "nog", "kla-", "gen?"],
        ["Hoe", "lang", "Uw", "gram-", "schap", "dra-", "gen,"],
        ["O", "HEER’,", "mijn", "toe-", "ver-", "laat?"]
