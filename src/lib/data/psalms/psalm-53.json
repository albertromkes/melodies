  "verses": [
    {
      "number": 1,
      "lines": [
        "De trotse dwaas zegt in zijn boos gemoed:",
        "„Daar is geen God.” Zij doven ’t licht derrede,",
        "En maken zich door gruwelijke zeden",
        "Afschuwelijk; daar is geen mens, die goed",
        "Op aarde doet."
      ],
      "syllables": [
        ["De", "trot-", "se", "dwaas", "zegt", "in", "zijn", "boos", "ge-", "moed:"],
        ["„Daar", "is", "geen", "God.”", "Zij", "do-", "ven", "’t licht", "der-", "re-", "de,"],
        ["En", "ma-", "ken", "zich", "door", "gru-", "we-", "lij-", "ke", "ze-", "den"],
        ["Af-", "schu-", "we-", "lijk;", "daar", "is", "geen", "mens,", "die", "goed"],
        ["Op", "aar-", "de", "doet."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "God, die het recht met kracht verdedigt, sloeg",
        "Van ’s hemels troon Zijn ogen naar beneden,",
        "Op Adams kroost’ doorzocht hun hart en zeden.",
        "Hij zag of zich geen mens verstandig droeg,",
        "En naar Hem vroeg."
      ],
      "syllables": [
        ["God,", "die", "het", "recht", "met", "kracht", "ver-", "de-", "digt,", "sloeg"],
        ["Van", "’s he-", "mels", "troon", "Zijn", "o-", "gen", "naar", "be-", "ne-", "den,"],
        ["Op", "A-", "dams", "kroos-", "t’door-", "zocht", "hun", "hart", "en", "ze-", "den."],
        ["Hij", "zag", "of", "zich", "geen", "mens", "ver-", "stan-", "dig", "droeg,"],
        ["En", "naar", "Hem", "vroeg."]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Hij zocht alom, maar ach; Hij vond ergeen;",
        "Want alle vlees is trouw’loos afgeweken;",
        "Het land is vol van stinkende gebreken.",
        "Geen sterveling wil ’t pad derdeugd betreên,",
        "Ja, zelfs niet één."
      ],
      "syllables": [
        ["Hij", "zocht", "al-", "om,", "maar", "ach;", "Hij", "vond", "erg-", "een;"],
        ["Want", "al-", "le", "vlees", "is", "trouw-", "’loos", "af-", "ge-", "we-", "ken;"],
        ["Het", "land", "is", "vol", "van", "stin-", "ken-", "de", "ge-", "bre-", "ken."],
        ["Geen", "ster-", "ve-", "ling", "wil", "’t pad", "der-", "deugd", "be-", "treên,"],
        ["Ja,", "zelfs", "niet", "één."]
      ]
    },
    {
      "number": 4,
      "lines": [
        "Heeft dan dit volk, dat groeit in euveldaân,",
        "Geen kennis? Neen, thans durven die ontzinden",
        "Met gulzigheid Mijn volk als brood verslinden;",
        "Zij roepen op hun godvergeten paân,",
        "Zelfs God niet aan."
      ],
      "syllables": [
        ["Heeft", "dan", "dit", "volk,", "dat", "groeit", "in", "eu-", "vel-", "daân,"],
        ["Geen", "ken-", "nis?", "Neen,", "thans", "dur-", "ven", "die", "ont-", "zin-", "den"],
        ["Met", "gul-", "zig-", "heid", "Mijn", "volk", "als", "brood", "ver-", "slin-", "den;"],
        ["Zij", "roe-", "pen", "op", "hun", "god-", "ver-", "ge-", "ten", "paân,"],
        ["Zelfs", "God", "niet", "aan."]
      ]
    },
    {
      "number": 5,
      "lines": [
        "Op ’t onverwachts zijn zij in angst gebracht,",
        "Want God heeft uw belegeraars doen vluchten,",
        "Hun beend’ ren zelfs verstrooid, die u deen zuchten,",
        "Hebt gij beschaamd; want God verwerpt, veracht",
        "Dit boos geslacht."
      ],
      "syllables": [
        ["Op", "’t on-", "ver-", "wachts", "zijn", "zij", "in", "angst", "ge-", "bracht,"],
        ["Want", "God", "heeft", "uw", "be-", "le-", "ge-", "raars", "doen", "vluch-", "ten,"],
        ["Hun", "beend’", "ren", "zelfs", "ver-", "strooid,", "die", "u", "deen", "zuch-", "ten,"],
        ["Hebt", "gij", "be-", "schaamd;", "want", "God", "ver-", "werpt,", "ver-", "acht"],
        ["Dit", "boos", "ge-", "slacht."]
      ]
    },
    {
      "number": 6,
      "lines": [
        "Och daalde ’t heil uit Sion spoedig neer",
        "Voor Israël! Als God Zijn volk uit lijden",
        "En banden redt, zal Jakob zich verblijden,",
        "En Israël, al juichend, geven d’ eer",
        "Aan zijnen HEER’."
      ],
      "syllables": [
        ["Och", "daal-", "de", "’t heil", "uit", "Si-", "on", "spoe-", "dig", "neer"],
        ["Voor", "Is-", "ra-", "ël!", "Als", "God", "Zijn", "volk", "uit", "lij-", "den"],
        ["En", "ban-", "den", "redt,", "zal", "Ja-", "kob", "zich", "ver-", "blij-", "den,"],
        ["En", "Is-", "ra-", "ël,", "al", "jui-", "chend,", "ge-", "ven", "d’eer"],
        ["Aan", "zij-", "nen", "HEER’."]
      ]
