        ["Die", "o-", "n-", "ge-", "temd,", "zich", "kreunt", "aan", "juk", "noch", "koorden,"],
        ["Vindt", "la-", "fe-", "nis", "aan", "ha-", "re", "fris-", "se", "boor-", "den."],
