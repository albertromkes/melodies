        ["U", "vei-", "lig", "doen", "ont-", "ko-", "-","men."],
        ["Hij", "is", "het,", "die", "uw", "le-", "ven", "redt;"],
        ["Gij", "hebt", "geen", "pest", "te", "schro-", "-","men."],
        ["Hij", "zal,", "in", "lijfs-", "en", "ziels-", "ge-", "vaar,"],
        ["U", "met", "Zijn", "vleug-", "’len", "dek-", "ken;"],
