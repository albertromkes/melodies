        ["’k Zoek", "hen", "ver-", "geefs,", "’k moet", "een-", "zaam", "we-", "nen;"],
        ["Al", "mijn", "be-", "ken-", "den", "zijn", "ver-", "dwe-", "nen."]
      ]
