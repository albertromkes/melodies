        ["Z’is", "als", "de", "dauw,", "die", "Her-", "mons", "kruin", "be-", "dekt,"],
        ["Die", "Si-", "ons", "top", "met", "vrucht-", "baar", "vocht", "be-", "sproeit,"],
        ["En", "op", "zijn", "ber-", "gen", "ne-", "der-", "vloeit."]
