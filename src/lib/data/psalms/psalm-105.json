      "syllables": [
        ["Looft,", "looft,", "ver-", "heugd", "den", "HEER’", "der", "He-", "ren;"],
        ["Aan-", "bidt", "Zijn", "Naam,", "en", "wilt", "Hem", "e-", "ren."],
        ["Doet", "Zij-", "ne", "glo-", "rie-", "rij-", "ke", "daân"],
