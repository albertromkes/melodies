      "syllables": [
        ["God", "zal", "Zijn", "waar-", "heid", "nim-", "mer", "kren-", "ken,"],
        ["Maar", "eeu-", "wig", "Zijn", "Ver-", "bond", "ge-", "den-", "ken."],
