  "verses": [
    {
      "number": 1,
      "lines": [
        "U mag men zalig heten,",
        "Dien ’s HEEREN vrees bekoort;",
        "Die, met een goed geweten,",
        "Steeds wandelt naar Zijn woord.",
        "Gij zult uw nooddruft vinden",
        "Door d’ arbeid van uw hand;",
        "Wat g’u moogt onderwinden,",
        "Komt naar uw wens tot stand."
      ],
      "syllables": [
        ["U", "mag", "men", "za-", "lig", "he-", "ten,"],
        ["Dien", "’s HEE-", "REN", "vrees", "be-", "koort;"],
        ["Die,", "met", "een", "goed", "ge-", "we-", "ten,"],
        ["Steeds", "wan-", "delt", "naar", "Zijn", "woord."],
        ["Gij", "zult", "uw", "nood-", "druft", "vin-", "den"],
        ["Door", "d’a-", "r-", "beid", "van", "uw", "hand;"],
        ["Wat", "g’u", "moogt", "o-", "n-", "der-", "win-", "den,"],
        ["Komt", "naar", "uw", "wens", "tot", "stand."]
      ]
    },
    {
      "number": 2,
      "lines": [
        "Uw echtvriendin zal bloeien,",
        "Gelijk een wijnstok tiert,",
        "Die, vruchtrijk onder ’t groeien,",
        "Uw huismuur dekt en siert.",
        "Niets zal uw welvaart stuiten;",
        "Uw kroost zal blij en fris,",
        "Als groen’ olijvenspruiten,",
        "Versieren uwen dis."
      ],
      "syllables": [
        ["Uw", "e-", "cht-", "vrien-", "din", "zal", "bloeien,"],
        ["Ge-", "lijk", "een", "wijn-", "stok", "tiert,"],
        ["Die,", "vrucht-", "rijk", "o-", "n-", "der", "’t groei-", "en,"],
        ["Uw", "huis-", "muur", "dekt", "en", "siert."],
        ["Niets", "zal", "uw", "wel-", "vaart", "stui-", "ten;"],
        ["Uw", "kroost", "zal", "blij", "en", "fris,"],
        ["Als", "groen’", "o-", "lij-", "ven-","sprui-", "ten,"],
        ["Ver-", "sie-", "ren", "u-", "wen", "dis."]
      ]
    },
    {
      "number": 3,
      "lines": [
        "Dit lot is u beschoren,",
        "Zo gij, met diep ontzag,",
        "Naar ’s HEEREN wet blijft horen;",
        "Voor u zal, dag aan dag,",
        "Het heil uit Sion vloeien:",
        "Gij zult, zolang gij leeft,",
        "Jeruzalem zien bloeien,",
        "’t Welk God Zijn zegen geeft."
      ],
      "syllables": [
        ["Dit", "lot", "is", "u", "be-", "scho-", "ren,"],
        ["Zo", "gij,", "met", "diep", "o-", "nt-", "zag,"],
        ["Naar", "’s HEE-", "REN", "wet", "blijft", "ho-", "ren;"],
        ["Voor", "u", "zal,", "dag", "aan", "dag,"],
        ["Het", "heil", "uit", "Si-", "on", "vloei-", "en:"],
        ["Gij", "zult,", "zo-", "lang", "gij", "leeft,"],
        ["Je-", "ru-", "za-", "lem", "zien", "bloei-", "en,"],
        ["’t Welk", "God", "Zijn", "ze-", "gen", "geeft."]
      ]
    },
    {
      "number": 4,
      "lines": [
        "Blijft gij op Hem betrouwen,",
        "Dan zult gij, op uw beê",
        "’t Kroost van uw kroost aanschouwen;",
        "In Israël zij vreê."
      ],
      "syllables": [
        ["Blijft", "gij", "op", "Hem", "be-", "trou-", "wen,"],
